{
  "ConnectionStrings": {
    "aspCoreBaseAuthorityConnectionString": "<DB_CONNECTION_STRING_AUTHORITY>",
    "aspCoreBaseConnectionString": "<DB_CONNECTION_STRING>"
  },
  "AppSettings": {
    "EncryptionDecryptionKey": "<ENCRYPTION_KEY>"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "IdentityProvider": {
    "Authority": "<IDENTITY_SERVER_AUTHORITY>",
    "ApiNme": "<IDENTITY_SERVER_API_NAME>",
    "ApiSecret": "<IDENTITY_SERVER_API_SECRET>",
    "RoleClaimType": "client_role",
    "EnableCaching": true,
    "CacheDuration": "00:30:00"
  }
}
